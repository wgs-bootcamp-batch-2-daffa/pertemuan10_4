<%- contentFor('root') %>
  <!-- begin::Navbar -->
  <%- include('partials/navbar') %>
    <!-- end::Navbar -->
    <!-- begin::Content -->
    <div id="about" class="row mt-5">
      <!-- begin::Title -->
      <div id="tittle" class="row my-5">
        <h1>
          <%= data.title %>
        </h1>
      </div>
      <!-- end::Title -->
      <!-- begin::Profile -->
      <div id="profile" class="row">
        <div class="text-center">
          <div class="card" style="width: 225px;">
            <img src="assets/putin.jpg" style="width:200px;height:200px;margin:auto" class="rounded-circle"
              alt="uraaa...">
            <div class="card-body">
              <h5 class="card-title">
                <%= data.name %>
              </h5>
              <p class="card-text">
                <%= data.occupation %>
              </p>
            </div>
          </div>
        </div>
      </div>
      <!-- end::Profile -->
    </div>
    <!-- end::Content -->